<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="/assets/css/font-awesome.min.css" rel="stylesheet" type="text/css"/>
        <link href="/assets/css/style.css" rel="stylesheet" type="text/css"/>
        <script src="/assets/js/jquery-3.1.1.min.js" type="text/javascript"></script>
        <script src="/assets/js/ui.js" type="text/javascript"></script>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    {% block javascripts %}
        <!--script
            src="https://code.jquery.com/jquery-2.2.4.js"
            integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI="
        crossorigin="anonymous"></script-->
        <!--<script src="/assets/js/jquery-3.1.1.min.js"></script>-->
    {% endblock %}
</head>
<body>
    <div class="wrapper login-bg">
        <header class="header">
            <div class="header-inner">

                <div class="logo" style="text-align: left;">
                    <a href="/">
                        <img src="/assets/images/aizan_logo2.png" width="120" alt=""/></a>
                </div>
            </div>
            
        </header>
        <div class="feature-list">
                <ul>
                    <li>
                        <a href="#">
                            <i class="fa fa-home"></i>
                            <span>Home</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-user"></i>
                            <span>Users</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-users"></i>
                            <span>Customer</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-list-alt"></i>
                            <span>Marketing Projects</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-gears"></i>
                            <span>Initiatives</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-bullhorn"></i>
                            <span>Campaign</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-address-book"></i>
                            <span>Campaign Contacts</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-envelope"></i>
                            <span>Psuedo Email Accounts</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-desktop"></i>
                            <span>Assign Psudeo</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-user-circle "></i>
                            <span>Agents</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-folder-open"></i>
                            <span>Agent Assignments</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-dollar"></i>
                            <span>Links</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-tachometer"></i>
                            <span>Link Rates</span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fa fa-list-alt"></i>
                            <span>DNS List</span>
                        </a>
                    </li>
                </ul>
            </div>
        {% for type, messages in app.session.flashbag.all() %}
            {% for message in messages %}
                {% if type == 'success' %}
                    <div style="color: green"> {{ message }} </div>
                {% else %}
                    <div style="color: red"> {{ message }} </div>

                {% endif %}
                {# Or even with css class rendering:
                  <div class="flash-{{type}}">{{message}}</div>
                 #}
            {% endfor %}
        {% endfor %}
    {% block body %}{% endblock %}
{% block footer %}{% endblock %}
</div>
<footer class="footer">
    <div class="left-footer">
        <p>&copy; aizan.com</p>
    </div>
    <div class="right-footer">
        <p>
            <a href="#">Privacy Policy</a>
        </p>
    </div>
</footer>
</body>
</html>
