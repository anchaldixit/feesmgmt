{# Genraral base template file #}
<!DOCTYPE html>
<html>
    <head>
        {% block title %}
            <title>Aizan</title>
        {% endblock %}
        {% block meta %}
            <meta charset="UTF-8">
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% endblock %}
        {% block stylesheet %}
            <link href="{{ asset('bundles/intelligentuser/css/font-awesome.min.css') }}" rel="stylesheet">
            <link href="{{ asset('bundles/intelligentuser/css/style.css') }}" rel="stylesheet">
        {% endblock %}
        {% block javascript %}
            <script src="{{ asset('bundles/intelligentuser/js/jquery-3.1.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/intelligentuser/js/ui.js') }}" type="text/javascript"></script>
        {% endblock %}
    </head>
    <body>
        <div class="wrapper login-bg">
            {% block header %}
                {{include('shared/header.html.twig')}}
            {% endblock %}
            {% block body %}{% endblock %}                    
            {% block footer %}
                {{include("shared/footer.html.twig")}}
            {% endblock %}
        </div>
    </body>
    {% block inlinejs %}{% endblock %}
</html>
