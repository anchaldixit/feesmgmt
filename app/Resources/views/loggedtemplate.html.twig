            <title>Aizan</title>
        {% endblock %}
        {% block meta %}
            <meta charset="UTF-8">
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% endblock %}
        {% block stylesheet %}
            <link href="{{ asset('assets/css/font-awesome.min.css') }}" rel="stylesheet">
            <link href="{{ asset('assets/css/style.css') }}" rel="stylesheet">
        {% endblock %}
        {% block javascript %}
            <script src="{{ asset('assets/js/jquery-3.1.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('assets/js/ui.js') }}" type="text/javascript"></script>
        {% endblock %}
    </head>
    <body>
        <div class="wrapper">
            <header class="header">
    <div class="header-inner">
        <div class="logo" style="text-align: left;">
            <a href="/">
                <img src="{{ asset('assets/images/aizan_logo2.png') }}" width="120" alt=""/></a>
        </div>
        <ul>
            <li><a class="b-btn" href="#"><span class="fa fa-plus-circle"></span>New</a></li>
            <li><a class="b-btn" href="#"><span class="fa fa-star"></span>Favorites</a></li>
            <li><a class="b-btn" href="#"><span class="fa fa-search"></span>Search</a></li>
            <li><a class="b-btn" href="#"><span class="fa fa-question-circle"></span>Help</a></li>
            <li><a class="b-btn" href="#"><span class="fa fa-exclamation-circle"></span>Alerts</a></li>
            <li>
                <a class="b-btn profile open-menu" href="#user_popup"><span class="fa fa-user"></span>{{ app.user.getName() }}<span class="fa-rt fa fa-caret-down"></span></a>
                <div class="PopMenu" id="user_popup">
                    <ul>
                        <li>
                            <h4>{{ app.user.getName() }}</h4>
                            <a class="e-mail" href="mailto:{{ app.user.getEmail() }}">{{ app.user.getEmail() }}</a>
                            <p><A href="/myPreferences">My Preferences</a></p>
                        </li>
                        <li>
                            <h4>Aizan</h4>
                            <span>My role in this app is {{ app.user.getRole().getName() }}</span>
                            <p><a href="#">My Personal setting in this app</a></p>
                        </li>
                        <li><a class="signout" href="/logout">Sign Out</a></li>
                    </ul>
                </div>
            </li>
        </ul>
    </div>
</header>
        
